    {
        require(path[0] == WETH, "UNIv2Router: INVALID_PATH");
        amounts = UNILibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= msg.value, "UNIv2Router: EXCESSIVE_INPUT_AMOUNT");
