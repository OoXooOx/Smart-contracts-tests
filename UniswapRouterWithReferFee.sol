pragma solidity 0.8.19;

contract referVault {
    address constant router = 0xCf7Ed3AccA5a467e9e704C703E8D87F634fB0Fc9;

    mapping(address => mapping(address => uint256)) public referAmounts;
    
    function setReferReward(address _refer, address _token, uint _amount) external {
        require(msg.sender==router, "invalid caller");
        referAmounts[_refer][_token]=_amount;
    }

    // **** REFER FUNCTIONS ****
    function referWithdraw(address token) external {
        require(token != address(0), "Wrong token");
        uint256 amount = referAmounts[msg.sender][token];
        require(amount != 0, "nothing withdraw");
        referAmounts[msg.sender][token] = 0;
        (bool success, bytes memory response) = address(token).call(
            abi.encodeWithSignature(
                "transfer(address,uint256)",
                msg.sender,
                amount)
            );
        require(success && (response.length == 0 || abi.decode(response, (bool))), "Failed send funds");
    }
}



